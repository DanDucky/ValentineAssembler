; this is a comment

;^ <- status flag name
BRA ^CRY #x // where x is defined with &

;% <- compiled register value
;ex:
MOV %ACC %X

;$ <- compiled number
;ex:
STOR %ACC $69

;! <- binary value as number
;ex:
STOR %ACC !00100010 ; must be 8 bits

;& <- save address as macro

;# <- subroutine address
;ex:
JMP #yobro

;. <- declare subroutine
.yobro
RTS

;> <- compiler instruction (starting addresses)
>BEGIN=0

;@ <- override subroutine
@interrupt
JMP #interrupt

;/ <- macro
/ADDRESSOFSTACK=29

;\ <- use macro
JMP $\ADDRESSOFSTACK ---> JMP $29

